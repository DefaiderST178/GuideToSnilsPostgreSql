<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="FrameGreen">#7C9867</Color>
    <Color x:Key="BackgroundGreen">#B4B471</Color>
    <Color x:Key="Dark">#948079</Color>
    <Color x:Key="TitleText">#231717</Color>
    <Color x:Key="MainText">#1F1010</Color>

    <SolidColorBrush x:Key="FrameGreenBrush" Color="{StaticResource FrameGreen}"/>
    <SolidColorBrush x:Key="BackgroundGreenBrush" Color="{StaticResource BackgroundGreen}"/>
    <SolidColorBrush x:Key="DarkBrush" Color="{StaticResource Dark}"/>
    <SolidColorBrush x:Key="TitleTextBrush" Color="{StaticResource TitleText}"/>
    <SolidColorBrush x:Key="MainTextBrush" Color="{StaticResource MainText}"/>

</ResourceDictionary>